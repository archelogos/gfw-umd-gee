        "/v1/forma250gfw": {
            "get": {
                "description": "Forma250 GFW data analysis, from Google's Earth Engine. Estimating the recent alerts, and area of clearing.",
                "tags": [
                    "FORMA250"
                ],
                "parameters": [
                    {
                        "name": "geostore",
                        "in": "query",
                        "description": "Hash of the geostore",
                        "required": true,
                        "type": "string"
                    },
                    {
                        "name": "period",
                        "in": "query",
                        "description": "Period of time (default '2001-01-01, 2017-02-01')",
                        "required": true,
                        "type": "string"
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Results obtained",
                        "schema": {
                            "$ref": "#/definitions/USEFORMA250"
                        }
                    },
                    "404": {
                        "description": "Geostore not found",
                        "schema": {
                            "$ref": "#/definitions/Errors"
                        }
                    },
                    "500": {
                        "description": "Unexpected error",
                        "schema": {
                            "$ref": "#/definitions/Errors"
                        }
                    },
                    "default": {
                        "description": "unexpected error",
                        "schema": {
                            "$ref": "#/definitions/Errors"
                        }
                    }
                }
            }
        },
